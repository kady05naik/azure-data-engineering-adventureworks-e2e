		{
			"name": "[concat(parameters('workspaceName'), '/CreateViews')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "-------------------------\n--Creating calendar view\n-------------------------\nCREATE VIEW gold.calendar\nAS\nSELECT\n*\nFROM\n    OPENROWSET(\n        BULK 'https://accstokad.blob.core.windows.net/silver/AdventureWorks_Calendar/',\n        FORMAT = 'PARQUET'\n    )as query1;\n\n\n-------------------------\n--Creating customers view\n-------------------------\nCREATE VIEW gold.customers\nAS\nSELECT \n*\nFROM\n    OPENROWSET(\n        BULK 'https://accstokad.blob.core.windows.net/silver/AdventureWorks_Customers/',\n        FORMAT= 'PARQUET'\n    )query1;\n\n\n----------------------------------\n--Creating product_categories view\n----------------------------------\nCREATE VIEW gold.product_categories\nAS\nSELECT \n*\nFROM\n    OPENROWSET(\n        BULK 'https://accstokad.blob.core.windows.net/silver/AdventureWorks_Product_Categories/',\n        FORMAT= 'PARQUET'\n    )query1;\n\n\n----------------------------------\n--Creating product_subcategories view\n----------------------------------\nCREATE VIEW gold.product_subcategories\nAS\nSELECT \n*\nFROM\n    OPENROWSET(\n        BULK 'https://accstokad.blob.core.windows.net/silver/AdventureWorks_Product_Subcategories/',\n        FORMAT= 'PARQUET'\n    )query1;\n\n\n----------------------------------\n--Creating products view\n----------------------------------\nCREATE VIEW gold.products\nAS\nSELECT \n*\nFROM\n    OPENROWSET(\n        BULK 'https://accstokad.blob.core.windows.net/silver/AdventureWorks_Products/',\n        FORMAT= 'PARQUET'\n    )query1;\n\n\n----------------------------------\n--Creating pro_returns view\n----------------------------------\nCREATE VIEW gold.pro_returns\nAS\nSELECT \n*\nFROM\n    OPENROWSET(\n        BULK 'https://accstokad.blob.core.windows.net/silver/AdventureWorks_Returns/',\n        FORMAT= 'PARQUET'\n    )query1;\n\n\n----------------------------------\n--Creating pro_returns view\n----------------------------------\nCREATE VIEW gold.pro_returns\nAS\nSELECT \n*\nFROM\n    OPENROWSET(\n        BULK 'https://accstokad.blob.core.windows.net/silver/AdventureWorks_Returns/',\n        FORMAT= 'PARQUET'\n    )query1;\n\n\n\n----------------------------------\n--Creating territories view\n----------------------------------\nCREATE VIEW gold.territories\nAS\nSELECT \n*\nFROM\n    OPENROWSET(\n        BULK 'https://accstokad.blob.core.windows.net/silver/AdventureWorks_Territories/',\n        FORMAT= 'PARQUET'\n    )query1;\n\n\n----------------------------------\n--Creating sales view\n----------------------------------\nCREATE VIEW gold.sales\nAS\nSELECT \n*\nFROM\n    OPENROWSET(\n        BULK 'https://accstokad.blob.core.windows.net/silver/AdventureWorks_Sales/',\n        FORMAT= 'PARQUET'\n    )query1;\n",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "myprodatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/createExTable')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"content": {
					"query": "CREATE DATABASE SCOPED CREDENTIAL cred_kad\nWITH\n    IDENTITY = 'Managed Identity';\n\n\nCREATE EXTERNAL DATA SOURCE source_silver\nWITH\n(\n    LOCATION = 'https://accstokad.blob.core.windows.net/silver',\n    CREDENTIAL= cred_kad\n);\n\n\nCREATE EXTERNAL DATA SOURCE source_gold\nWITH\n(\n    LOCATION = 'https://accstokad.blob.core.windows.net/gold',\n    CREDENTIAL= cred_kad\n);\n\n\nCREATE EXTERNAL FILE FORMAT format_parquet\nWITH\n(\n    FORMAT_TYPE=PARQUET,\n    DATA_COMPRESSION='org.apache.hadoop.io.compress.SnappyCodec'\n);\n\n\nCREATE EXTERNAL TABLE gold.extsales\nWITH\n    (\n        LOCATION='extsales',\n        DATA_SOURCE=source_gold,\n        FILE_FORMAT=format_parquet\n    )\n    AS\n    SELECT * FROM gold.sales;\n\n\n",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "myprodatabase",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		}
